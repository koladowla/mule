<settings>
  <servers>
    <server>
      <id>Dowla</id>
      <username>YOUR_ANYPOINT_USERNAME</username>
      <password>YOUR_ANYPOINT_PASSWORD</password>
    </server>
  </servers>

  <profiles>
    <profile>
      <id>anypoint</id>
      <properties>
        <anypoint.clientId>YOUR_CLIENT_ID</anypoint.clientId>
        <anypoint.clientSecret>YOUR_CLIENT_SECRET</anypoint.clientSecret>
      </properties>
    </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>anypoint</activeProfile>
  </activeProfiles>
</settings>
